{
  "rules": {
    "dporians": {
      ".write": "auth != null",
      "$uid": {
        ".read": "auth != null && auth.uid === $uid"
      }
    },
    "groups": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "instructions": {
      ".read": "auth != null",
      ".write": false
    },
    "stimuli": {
      ".read": "auth != null",
      ".write": false
    },
    "vacancies": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}